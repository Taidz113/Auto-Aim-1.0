local Players = game:GetService(tuVslmozExBQltbuRDaXpJMlsrvgDuuCYxQEhLMKOaFZsODYgvmjCBxWgbVwMytczXqBA('aHrZJOyrNHtPccZzAAgEENWbhImEfSpjsRlQYKknmzdVoKdomSueqvLUGxheWVycw=='))
local LocalPlayer = Players.LocalPlayer
local Camera = game:GetService(tuVslmozExBQltbuRDaXpJMlsrvgDuuCYxQEhLMKOaFZsODYgvmjCBxWgbVwMytczXqBA('ZeQxjRexkEWkOgQasKKkmnVDAheXnApXTPqcPxTaWpeWsDMZPXfsxwxV29ya3NwYWNl')).CurrentCamera

-- Error handling function
local function handleError(err)
    warn(tuVslmozExBQltbuRDaXpJMlsrvgDuuCYxQEhLMKOaFZsODYgvmjCBxWgbVwMytczXqBA('cFXQLNsDsgyHYQDJAPGHyOWhBwmUElWTPCmHJgQNrzhVSjVUHBMHCZxQW4gZXJyb3Igb2NjdXJyZWQ6IA==') .. tostring(err))
end

-- Function to create ESP
local function createESP(player)
    local espBox = Instance.new(tuVslmozExBQltbuRDaXpJMlsrvgDuuCYxQEhLMKOaFZsODYgvmjCBxWgbVwMytczXqBA('KJidDEVoDMzsKRPsKQGzBPWdoKbakPfDTHGypJvmcJbrFkMKToNhfdUQm94SGFuZGxlQWRvcm5tZW50'))
    espBox.Size = Vector3.new(2, 5, 1) -- Size of the box
    espBox.Color3 = player.TeamColor.Color -- Set color based on team
    espBox.AlwaysOnTop = true
    espBox.ZIndex = 5
    espBox.Parent = player.Character:FindFirstChild(tuVslmozExBQltbuRDaXpJMlsrvgDuuCYxQEhLMKOaFZsODYgvmjCBxWgbVwMytczXqBA('iHUxHXOwXSKsjRydaWsXQMjHEELAKeWVMJTsCcGVuDgFoSgaNoBarJjSHVtYW5vaWRSb290UGFydA=='))
end

-- Function to aim at the nearest enemy
local function autoAim()
    local closestEnemy = nil
    local closestDistance = math.huge

    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild(tuVslmozExBQltbuRDaXpJMlsrvgDuuCYxQEhLMKOaFZsODYgvmjCBxWgbVwMytczXqBA('UOLpKbDivpusiEkgCSrYhEghKgQobVDAkoZTxoidjAUDwPMGXyGULMhSHVtYW5vaWRSb290UGFydA==')) then
            local distance = (LocalPlayer.Character.HumanoidRootPart.Position - player.Character.HumanoidRootPart.Position).magnitude
            if distance < closestDistance then
                closestDistance = distance
                closestEnemy = player
            end
        end
    end

    if closestEnemy then
        local targetPosition = closestEnemy.Character.Head.Position
        local cameraCFrame = CFrame.new(Camera.CFrame.Position, targetPosition)
        Camera.CFrame = cameraCFrame
    end
end

-- Main execution block
local function main()
    pcall(function()
        -- Create ESP for all players
        for _, player in pairs(Players:GetPlayers()) do
            if player ~= LocalPlayer then
                createESP(player)
            end
        end

        -- Continuously aim at the nearest enemy
        while true do
            autoAim()
            wait(0.1) -- Adjust the wait time as necessary
        end
    end)
end

-- Start the script
main()
    
